<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dcterms="http://purl.org/dc/terms/"
         xmlns:foaf="http://xmlns.com/foaf/0.1/"
         xmlns:uni="http://example.org/university#"
         xml:base="http://example.org/university">

    <!-- Ontology Metadata -->
    <owl:Ontology rdf:about="http://example.org/university">
        <dc:title xml:lang="en">University Ontology</dc:title>
        <dc:description xml:lang="en">A simple ontology describing university concepts including students, professors, courses, and departments.</dc:description>
        <dc:creator>LODE Test User</dc:creator>
        <dcterms:created>2024-01-15</dcterms:created>
        <dcterms:modified>2024-01-20</dcterms:modified>
        <owl:versionInfo>1.0</owl:versionInfo>
        <rdfs:comment xml:lang="en">This ontology is designed for testing LODE documentation generation.</rdfs:comment>
    </owl:Ontology>

    <!-- Classes -->
    <owl:Class rdf:about="http://example.org/university#Person">
        <rdfs:label xml:lang="en">Person</rdfs:label>
        <rdfs:comment xml:lang="en">A human being associated with the university.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/university#Student">
        <rdfs:label xml:lang="en">Student</rdfs:label>
        <rdfs:comment xml:lang="en">A person who is enrolled in courses at the university.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/university#Person"/>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/university#Professor">
        <rdfs:label xml:lang="en">Professor</rdfs:label>
        <rdfs:comment xml:lang="en">A person who teaches courses and conducts research at the university.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/university#Person"/>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/university#Course">
        <rdfs:label xml:lang="en">Course</rdfs:label>
        <rdfs:comment xml:lang="en">An educational program or class offered by the university.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/university#Department">
        <rdfs:label xml:lang="en">Department</rdfs:label>
        <rdfs:comment xml:lang="en">An academic division within the university that offers courses and degrees.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/university#Degree">
        <rdfs:label xml:lang="en">Degree</rdfs:label>
        <rdfs:comment xml:lang="en">An academic qualification awarded by the university.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://example.org/university#enrolledIn">
        <rdfs:label xml:lang="en">enrolled in</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a student to a course they are taking.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Student"/>
        <rdfs:range rdf:resource="http://example.org/university#Course"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/university#teaches">
        <rdfs:label xml:lang="en">teaches</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a professor to a course they teach.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Professor"/>
        <rdfs:range rdf:resource="http://example.org/university#Course"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/university#offeredBy">
        <rdfs:label xml:lang="en">offered by</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a course to the department that offers it.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Course"/>
        <rdfs:range rdf:resource="http://example.org/university#Department"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/university#belongsTo">
        <rdfs:label xml:lang="en">belongs to</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a professor to their department.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Professor"/>
        <rdfs:range rdf:resource="http://example.org/university#Department"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/university#hasDegree">
        <rdfs:label xml:lang="en">has degree</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a person to a degree they have earned.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Person"/>
        <rdfs:range rdf:resource="http://example.org/university#Degree"/>
    </owl:ObjectProperty>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="http://example.org/university#studentId">
        <rdfs:label xml:lang="en">student ID</rdfs:label>
        <rdfs:comment xml:lang="en">The unique identifier for a student.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Student"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/university#courseName">
        <rdfs:label xml:lang="en">course name</rdfs:label>
        <rdfs:comment xml:lang="en">The title or name of a course.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Course"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/university#credits">
        <rdfs:label xml:lang="en">credits</rdfs:label>
        <rdfs:comment xml:lang="en">The number of credit hours for a course.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Course"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/university#email">
        <rdfs:label xml:lang="en">email</rdfs:label>
        <rdfs:comment xml:lang="en">The email address of a person.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/university#yearEstablished">
        <rdfs:label xml:lang="en">year established</rdfs:label>
        <rdfs:comment xml:lang="en">The year when a department was established.</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Department"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
    </owl:DatatypeProperty>

    <!-- Sample Individuals -->
    <uni:Department rdf:about="http://example.org/university#ComputerScience">
        <rdfs:label xml:lang="en">Computer Science Department</rdfs:label>
        <rdfs:comment xml:lang="en">The department responsible for computer science education and research.</rdfs:comment>
        <uni:yearEstablished rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">1985</uni:yearEstablished>
    </uni:Department>

    <uni:Professor rdf:about="http://example.org/university#DrSmith">
        <rdfs:label xml:lang="en">Dr. John Smith</rdfs:label>
        <rdfs:comment xml:lang="en">Professor of Computer Science specializing in artificial intelligence.</rdfs:comment>
        <uni:email>j.smith@university.edu</uni:email>
        <uni:belongsTo rdf:resource="http://example.org/university#ComputerScience"/>
    </uni:Professor>

    <uni:Course rdf:about="http://example.org/university#CS101">
        <rdfs:label xml:lang="en">Introduction to Computer Science</rdfs:label>
        <rdfs:comment xml:lang="en">A foundational course covering basic programming concepts.</rdfs:comment>
        <uni:courseName>Introduction to Computer Science</uni:courseName>
        <uni:credits rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</uni:credits>
        <uni:offeredBy rdf:resource="http://example.org/university#ComputerScience"/>
    </uni:Course>

    <uni:Student rdf:about="http://example.org/university#JaneStudent">
        <rdfs:label xml:lang="en">Jane Student</rdfs:label>
        <rdfs:comment xml:lang="en">A computer science student in her second year.</rdfs:comment>
        <uni:studentId>CS2024001</uni:studentId>
        <uni:email>jane.student@university.edu</uni:email>
        <uni:enrolledIn rdf:resource="http://example.org/university#CS101"/>
    </uni:Student>

</rdf:RDF>
